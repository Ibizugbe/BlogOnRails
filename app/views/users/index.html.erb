<main>
  <h1 class="author-header">Authors</h1>
  <section>
    <ul class='authors'>
      <% @users.each do |user| %>
        <a id='Time' href='/users/<%= user.id %>'>
          <%=render 'partials/user', user:user %>
        </a>
      <% end %>
    </ul>
    <%= link_to 'Create post', new_user_post_path(@user) %>
  </section>
</main>
